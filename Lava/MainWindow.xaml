                    </Button>

                    <Image x:Name="Img_But_Door2" Grid.Column="1" Source="Images\Grey кнопка.png" Margin="149,378,9,-83"/>
                    <Button Style="{StaticResource MyButton}" x:Name="Fuse_Door2" VerticalAlignment="Top" FontSize="11" FontFamily="Arial" Height="41" Cursor="Hand" BorderBrush="{x:Null}" Click="Fuse_Door_Click2" FontWeight="Bold" Foreground="Gray" Grid.Column="1" Margin="149,466,12,0">
                        <Button.Content>
                            <TextBlock Text="Основная горелка" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" HorizontalAlignment="Center" Width="127"/>
                        </Button.Content>
                    </Button>
                    <!--Газовые баллоны-->
                    <Viewbox HorizontalAlignment="Center" Margin="42,-37,34,239" Grid.Column="2" Width="211">
                        <TextBlock HorizontalAlignment="Center" TextAlignment="Center" FontSize="15" Text="Имитатор горящих газовых баллонов" TextWrapping="Wrap" FontWeight="Bold" Width="143" VerticalAlignment="Center"></TextBlock>
                    </Viewbox>

                    <Viewbox HorizontalAlignment="Center" Grid.ColumnSpan="3" Grid.Column="1" Margin="254,38,335,-23" Width="375">
                        <Image x:Name="Cylinders" HorizontalAlignment="Left" Height="168" VerticalAlignment="Top" Width="208" Source="Images\Горящие баллоны_1.png" Cursor="Hand"/>
                    </Viewbox>

                    <Image x:Name="Img_But_Cylinders" Grid.Column="2" Source="Images\Grey кнопка.png" Margin="18,202,142,-259"/>
                    <Button Style="{StaticResource MyButton}" x:Name="Fuse_Cylinders" VerticalAlignment="Top" FontSize="11" FontFamily="Arial" Height="42" Cursor="Hand" BorderBrush="{x:Null}" Click="Fuse_Cylinders_Click" FontWeight="Bold" Margin="18,466,144,-180" Grid.Column="2">
                        <Button.Content>
                            <TextBlock Text="Запальная горелка" TextWrapping="Wrap" FontSize="17" TextAlignment="Center" Width="125"/>
                        </Button.Content>
                    </Button>

                    <Image x:Name="Img_But_Cylinders2" Grid.Column="2" Source="Images\Grey кнопка.png" Margin="150,202,10,-259"/>
                    <Button Style="{StaticResource MyButton}" x:Name="Fuse_Cylinders2" VerticalAlignment="Top" FontSize="11" Foreground="Gray" FontFamily="Arial" Height="42" Cursor="Hand" BorderBrush="{x:Null}" Click="Fuse_Cylinders_Click2" FontWeight="Bold" Margin="150,466,12,0" Grid.Column="2">
                        <Button.Content>
                            <TextBlock Text="Основная горелка" TextWrapping="Wrap" FontSize="17" TextAlignment="Center" Width="125"/>
                        </Button.Content>
                    </Button>
                    <!--Газовый трубопровод-->
                    <Viewbox HorizontalAlignment="Center" Margin="77,-61,57,213" Grid.Column="3" Width="259">
                        <TextBlock HorizontalAlignment="Center" TextAlignment="Center" FontSize="15" Text="Имитатор горящего газового трубопровода" TextWrapping="Wrap" FontWeight="Bold" Width="173" VerticalAlignment="Center"></TextBlock>
                    </Viewbox>

                    <Viewbox HorizontalAlignment="Center" Grid.Column="2" Width="404" Margin="219,10,57,-83" Grid.ColumnSpan="2">
