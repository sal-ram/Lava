                        <StackPanel Orientation="Horizontal" Grid.Column="1" Margin="0,0,0,6">
                            <Label Content="Режим управления: " FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Foreground="Black"/>
                            <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top" Background="#FFE2E1E1" Height="27" Margin="0" Width="115">
                                <Viewbox>
                                    <Label x:Name="Mode_Value_text" Content="Автоматический" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                                </Viewbox>
                            </Border>
                        </StackPanel>
                        <Label Content="Давление газа в системе" FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Foreground="Black"/>
                        <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top" Background="#FFE2E1E1" Height="27" Margin="100, 5" Width="78">
                            <Viewbox>
                                <Label x:Name="Monometr_Value_text" Content="0,4 MPa" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>
