                        </Border>
                    </Viewbox>
                    <Viewbox HorizontalAlignment="Center" Grid.Column="0" Margin="-122,464,144,10" Width="270">
                        <Button Style="{StaticResource MyButton}" x:Name="ButtonSvet" Foreground="{x:Null}" ScrollViewer.VerticalScrollBarVisibility="Disabled" Background="#00000000"/>
                    </Viewbox>
