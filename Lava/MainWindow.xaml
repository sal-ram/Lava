<Window x:Class="Lava.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lava"
        mc:Ignorable="d"
        Title="MainWindow"
        ResizeMode="NoResize"
        WindowStyle="None"
        WindowState="Maximized"
        Width="1680" Height="1050">

    <Window.Resources>
        <Storyboard x:Key="FlashButton" RepeatBehavior="Forever">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="button">
                <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="0.2"/>
                <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="ChangeColor">
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)" Storyboard.TargetName="button">
                <EasingColorKeyFrame KeyTime="0:0:0.5" Value="Red"/>
            </ColorAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="ChangeColor2">
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)" Storyboard.TargetName="button">
                <EasingColorKeyFrame KeyTime="0:0:0.5" Value="White"/>
            </ColorAnimationUsingKeyFrames>
        </Storyboard>
    </Window.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="1049*"/>
        </Grid.RowDefinitions>
        <Grid.Background>
            <ImageBrush ImageSource="Images/Интерфейс Лава2.png"/>
        </Grid.Background>

        <Grid VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,8,10,0" Height="47" Width="137" Grid.Row="1">
            <Button Style="{StaticResource MyButton}" Content="-" Cursor="Hand" FontSize="16" MinWidth="25" MinHeight="25" VerticalAlignment="Top" Background="#FF959393" Foreground="White" RenderTransformOrigin="0.5,0.5" FontWeight="Bold" FontStyle="Italic" Click="Button_Click_1" Height="40" Width="40"/>
            <Button Style="{StaticResource MyButton}" Content="X" HorizontalAlignment="Right" Cursor="Hand" FontSize="16" MinWidth="25" MinHeight="25" VerticalAlignment="Top" Background="#FFE64343" Foreground="White" Click="Button_Click" Width="40" Height="40"/>
        </Grid>

        <Grid Margin="10,55.5,10,10" Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="101*"/>
                <ColumnDefinition Width="336*"/>
            </Grid.ColumnDefinitions>

            <Grid.Background>
                <ImageBrush ImageSource="Images/Интерфейс Лава4.png"/>
            </Grid.Background>

            <Grid Grid.Column="0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="372*"></RowDefinition>
                    <RowDefinition Height="290*"></RowDefinition>
                    <RowDefinition Height="557*"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Viewbox>
                    <StackPanel Orientation="Vertical">
                        <Border x:Name="AlarmBorder" CornerRadius="12" Grid.Row="0" BorderThickness="2" VerticalAlignment="Top" RenderTransformOrigin="0.468,0.361" Background="#FF5AC155" Height="46" Margin ="10, 6, 2, 1" >
                            <Viewbox>
                                <Label x:Name="AlarmText" Content="Состояние: исправно" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  Margin="-2" VerticalAlignment="Center" FontSize="13" FontFamily="Arial" FontWeight="Bold" Foreground="White" Height="38" BorderBrush="#FF555555" Background="{x:Null}"/>
                            </Viewbox>
                        </Border>
                        <Border Margin ="10, 0, 2, 4" CornerRadius="12" Grid.Row="0" BorderThickness="2,3,2,2" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top"  Background="#FFD82F2F" Height="46">
                            <Viewbox>
                                <Label Content="Сброс времени тренировки" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center"  FontSize="13" FontFamily="Arial" FontWeight="Bold" Foreground="White" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>
                        <Label Content="Время:" FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Foreground="Black" Height="26"/>
                        <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top" Background="#FFE2E1E1" Height="27" Margin="100, 0" Width="78">
                            <Viewbox>
                                <Label x:Name="Time" Content="11:05" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="20" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>

                        <Label Grid.Row="0" Content="Время тренировки:" HorizontalContentAlignment="Center" FontSize="15" FontWeight="Bold" FontFamily="Arial" Foreground="Black"/>
                        <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top"  Background="#FFE2E1E1" Height="27" Margin="70, 0">
                            <Viewbox>
                                <Label x:Name="TimeTraining" Content="00:01:05" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center"  FontSize="20" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>
                    </StackPanel>
                </Viewbox>

                <Viewbox Grid.Row="1">
                    <StackPanel Orientation="Vertical" Grid.Row="1" VerticalAlignment="Center"  Margin="10,40,10,0">
                        <Label Content="Температура" FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Foreground="Black"/>
                        <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top" Background="#FFE2E1E1" Height="27" Margin="100, 0" Width="78">
                            <Viewbox>
                                <Label x:Name="GazhranZoneTemp" Content="35 C" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>

                        <Label Content="Концентрация газа" FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Foreground="Black"/>
                        <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top" Background="#FFE2E1E1" Height="27" Margin="100, 0" Width="78">
                            <Viewbox>
                                <Label x:Name="GasAnalyzerGazohranZonePercent" Content="50 %" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>
                    </StackPanel>
                </Viewbox>

                <Viewbox Grid.Row="2" VerticalAlignment="Top" Margin="10,65,10,0">
                    <StackPanel Orientation="Vertical" Grid.Row="1" Height="150">
                        <StackPanel Orientation="Horizontal" Grid.Column="1" Margin="0,0,0,6">
                            <Label Content="Режим управления: " FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Foreground="Black"/>
                            <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top" Background="#FFE2E1E1" Height="27" Margin="0" Width="115">
                                <Viewbox>
                                    <Label x:Name="Mode_Value_text" Content="Автоматический" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                                </Viewbox>
                            </Border>
                        </StackPanel>
                        <Label Content="Давление газа в системе" FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Foreground="Black"/>
                        <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top" Background="#FFE2E1E1" Height="27" Margin="100, 5" Width="78">
                            <Viewbox>
                                <Label x:Name="Monometr_Value_text" Content="0,4 MPa" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>
                    </StackPanel>
                </Viewbox>
                <Image Grid.Row="2" x:Name="Monometr" HorizontalAlignment="Center" Height="215" VerticalAlignment="Top" Width="204" Source="Images/Манометр.png" Cursor="Hand" Margin="90,224,90,0"/>
                <Image Grid.Row="2" x:Name="Monometr_Strelka" HorizontalAlignment="Center" Height="97" VerticalAlignment="Top" Width="21" Source="Images/Стрелка.png" Cursor="Hand" RenderTransformOrigin="0.512,0.692" Margin="179,259,184,0">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleY="1.015" ScaleX="0.985"/>
                            <SkewTransform AngleY="-1.818"/>
                            <RotateTransform Angle="-136.244"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
            </Grid>

            <Grid Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <Grid Grid.Row="0" Grid.Column="0" Margin="10,0,10,469" Grid.RowSpan="2">
                    <Grid.RowDefinitions>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="298*"></ColumnDefinition>
                        <ColumnDefinition Width="290*"></ColumnDefinition>
                        <ColumnDefinition Width="293*"></ColumnDefinition>
                        <ColumnDefinition Width="401*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <!--Рубильник-->
                    <Viewbox Grid.Column="0" Margin="0,74,0,10">
                        <StackPanel Width="1785" Height="2897">
                            <Label Content="Рубильник" Margin="0,130,0,0" FontSize="150" FontFamily="Arial" HorizontalAlignment="Center" FontWeight="Bold" Foreground="Black" BorderBrush="#FF555555" Background="{x:Null}"/>
                            <Image x:Name="Svet" HorizontalAlignment="Center" VerticalAlignment="Center" Source="Images\Силовой Ящик_вкл.png" Cursor="Hand" Height="1948"/>
                            <Border x:Name="BorderSvet" BorderThickness="1" BorderBrush="Black" CornerRadius="30" RenderTransformOrigin="0.498,0.479" Background="#FFFFBB00" Height="260" Margin="177,360,172,0">
                                <Button Style="{StaticResource MyButton}" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" ScrollViewer.VerticalScrollBarVisibility="Disabled" Background="#00000000" Margin="0,0,0,-1">
                                    <TextBlock x:Name="Voltage" Text="Под напряжением" FontSize="125" TextAlignment="Center" TextWrapping="Wrap" Width="1360" Height="190"/>
                                </Button>
                            </Border>
                        </StackPanel>
                    </Viewbox>

                    <Viewbox Grid.Column="1" Margin="0,74,0,10">
                        <StackPanel Width="2605" Height="4613">
                            <!--Имитатор горящей двери-->
                            <TextBlock TextAlignment="Center" TextWrapping="Wrap" FontSize="200" Text="Имитатор горящей двери" FontWeight="Bold"  VerticalAlignment="Top" Margin="239,190,304,0" Height="495"/>
                            <Image x:Name="Door" HorizontalAlignment="Center" Height="2695" VerticalAlignment="Top" Width="3295" Source="Images\Дверь_1.png" Cursor="Hand" Margin="-330,0,-360,20"/>
                            <Grid Height="710" Margin="0,500,0,0">
                                <Image x:Name="Img_But_Door" Source="Images\Grey кнопка.png" Margin="64,299,1337,-12" Width="1204"/>
                                <Button Style="{StaticResource MyButton}" x:Name="Fuse_Door" VerticalAlignment="Center" FontSize="11" FontFamily="Arial" Height="404" Cursor="Hand" BorderBrush="{x:Null}" Click="Fuse_Door_Click" FontWeight="Bold" Margin="83,306,1357,0">
                                    <TextBlock Text="Запальная горелка" TextWrapping="Wrap" FontSize="171" TextAlignment="Center" Width="1165" Height="404"/>
                                </Button>

                                <Image x:Name="Img_But_Door2" Source="Images\Grey кнопка.png" Margin="1382,299,19,0" Width="1204"/>
                                <Button Style="{StaticResource MyButton}" x:Name="Fuse_Door2" FontSize="11"  VerticalAlignment="Center" FontFamily="Arial" Cursor="Hand" BorderBrush="{x:Null}" Click="Fuse_Door_Click2" FontWeight="Bold" Foreground="Gray" Margin="1394,306,25,20">
                                    <TextBlock Text="Основная горелка" FontSize="171" TextWrapping="Wrap" TextAlignment="Center" HorizontalAlignment="Center" Width="1138" Height="384"/>
                                </Button>
                            </Grid>
                        </StackPanel>
                    </Viewbox>
                    <!--Газовые баллоны-->
                    <Viewbox Grid.Column="2" Margin="0,74,0,10">
                        <StackPanel Width="6194" Height="9117">
                            <TextBlock HorizontalAlignment="Center" TextAlignment="Center" FontSize="400" Text="Имитатор горящих газовых баллонов" TextWrapping="Wrap" FontWeight="Bold" Width="4408" VerticalAlignment="Center" Margin="1017,250,769,0" Height="1072"/>
                            <Image x:Name="Cylinders" HorizontalAlignment="Center" Height="7398" VerticalAlignment="Top" Width="5995" Source="Images\Горящие баллоны_1.png" Cursor="Hand" Margin="88,0,111,0"/>
                            <Grid Margin="0,-1500,0,0" Height="1733">
                                <Image x:Name="Img_But_Cylinders" Source="Images\Grey кнопка.png" Margin="686,1032,3106,-186" />
                                <Button Style="{StaticResource MyButton}" x:Name="Fuse_Cylinders"  VerticalAlignment="Center" FontSize="11" FontFamily="Arial" Height="813" Cursor="Hand" BorderBrush="{x:Null}" Click="Fuse_Cylinders_Click" FontWeight="Bold" Margin="675,1073,3132,-153">
                                    <TextBlock Text="Запальная горелка" TextWrapping="Wrap" FontSize="317" TextAlignment="Center" Width="2377" Height="784"/>
                                </Button>

                                <Image x:Name="Img_But_Cylinders2" Source="Images\Grey кнопка.png" Margin="3304,1032,467,-187"/>
                                <Button Style="{StaticResource MyButton}" x:Name="Fuse_Cylinders2"  VerticalAlignment="Center" FontSize="11" Foreground="Gray" FontFamily="Arial" Height="892" Cursor="Hand" BorderBrush="{x:Null}" Click="Fuse_Cylinders_Click2" FontWeight="Bold" Margin="3304,1019,502,-178">
                                    <TextBlock Text="Основная горелка" TextWrapping="Wrap" FontSize="317" TextAlignment="Center" Width="2306" Height="805"/>
                                </Button>
                            </Grid>
                        </StackPanel>
                    </Viewbox>
                    <Viewbox Grid.Column="3" Margin="0,83,0,10">
                        <StackPanel Orientation="Vertical" Height="1315" Width="1336">
                            <!--Газовый трубопровод-->
                            <TextBlock HorizontalAlignment="Center" TextAlignment="Center" FontSize="65" Text="Имитатор горящего газового трубопровода" TextWrapping="Wrap" FontWeight="Bold" Width="1264" VerticalAlignment="Center"></TextBlock>
                            <StackPanel Orientation="Horizontal" Height="1139">
                                <!--Нижний фланец-->
                                <Image x:Name="Flange" HorizontalAlignment="Left" Height="1065" Margin="-200,0,0,0" VerticalAlignment="Top" Width="1137" Source="Images\Трубы_1.png" Cursor="Hand"/>
                                <!--Нижний фланец-->
                                <Grid Width="599" Margin="-200,113,0,146">
                                    <Border Margin="90,455,48,22" BorderBrush="Black" BorderThickness="2" CornerRadius="30">
                                        <Grid Margin="0,0,-1,0">
                                            <TextBlock HorizontalAlignment="Center" TextAlignment="Center" FontSize="35" TextWrapping="Wrap" FontWeight="Bold" Width="335" VerticalAlignment="Center" Margin="62,21,61,330" Height="48" Padding="0" ScrollViewer.CanContentScroll="True"><Run Text="Нижний "/><Run Text="фланец"/></TextBlock>
                                            <Image x:Name="Img_But_Flange_Low" Source="Images\Grey кнопка.png" Margin="62,-25,47,44"/>
                                            <Button Style="{StaticResource MyButton}" x:Name="Fuse_Flange_Low" VerticalAlignment="Center" FontFamily="Arial" Height="115" Cursor="Hand" Click="Fuse_Flange_Click_Low" BorderBrush="{x:Null}" FontWeight="Bold" Margin="62,113,47,171">
                                                <TextBlock Text="Запальная горелка" TextWrapping="Wrap" FontSize="43" TextAlignment="Center" Width="332" Height="109"/>
                                            </Button>

                                            <Image x:Name="Img_But_Flange2_Low" Source="Images\Grey кнопка.png" Margin="62,228,47,10"/>
                                            <Button Style="{StaticResource MyButton}" x:Name="Fuse_Flange2_Low" FontSize="11" FontFamily="Arial" Cursor="Hand" Click="Fuse_Flange_Click2_Low" BorderBrush="{x:Null}" FontWeight="Bold" Foreground="Gray" Margin="62,255,47,33">
                                                <TextBlock Text="Основная горелка" FontSize="43" TextWrapping="Wrap" TextAlignment="Center" HorizontalAlignment="Center" Width="339" Height="111"/>
                                            </Button>
                                        </Grid>
                                    </Border>

                                    <!--Верхний фланец-->
                                    <Border Margin="90,0,48,480" BorderBrush="Black" BorderThickness="2" CornerRadius="30">
                                        <Grid>
                                            <TextBlock HorizontalAlignment="Center" TextAlignment="Center" FontSize="35" TextWrapping="Wrap" FontWeight="Bold" Width="316" VerticalAlignment="Center" Margin="77,10,64,337" Height="49" Padding="0" ScrollViewer.CanContentScroll="True"><Run  Text="Верхний "/><Run Text="фланец"/></TextBlock>
                                            <Image x:Name="Img_But_Flange_High" Source="Images\Grey кнопка.png" Margin="68,26,45,113"/>
                                            <Button Style="{StaticResource MyButton}" x:Name="Fuse_Flange_High"  VerticalAlignment="Center" FontSize="11" FontFamily="Arial" Height="111" Cursor="Hand" Click="Fuse_Flange_Click_High" BorderBrush="{x:Null}" FontWeight="Bold" Margin="68,99,54,186">
                                                <TextBlock Text="Запальная горелка" TextWrapping="Wrap" FontSize="45" TextAlignment="Center" Width="325" Height="102"/>
                                            </Button>

                                            <Image x:Name="Img_But_Flange2_High" Source="Images\Grey кнопка.png" Margin="68,144,45,-55"/>
                                            <Button Style="{StaticResource MyButton}" x:Name="Fuse_Flange2_High"  VerticalAlignment="Center" FontSize="11" FontFamily="Arial" Height="107" Cursor="Hand" Click="Fuse_Flange_Click2_High" BorderBrush="{x:Null}" FontWeight="Bold" Foreground="Gray" Margin="77,242,55,47">
                                                <TextBlock Text="Основная горелка" FontSize="45" TextWrapping="Wrap" TextAlignment="Center" HorizontalAlignment="Center" Width="325" Height="107"/>
                                            </Button>
                                        </Grid>
                                    </Border>
                                </Grid>
                            </StackPanel>
                        </StackPanel>
                    </Viewbox>

                    <Viewbox VerticalAlignment="Top" Margin="2,19,-139,0" Height="59" Grid.Column="3">
                        <StackPanel Orientation="Horizontal" Width="331" Height="40" >
                            <Label Content="Концентрация газа" FontSize="15" FontWeight="Bold" FontFamily="Arial"  VerticalContentAlignment="Center" Margin= "0, 0, 10, 0" Foreground="Black"/>
                            <Border CornerRadius="12" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Center" Background="#FFE2E1E1" Height="28" Width="78" Margin="0,6">
                                <Viewbox>
                                    <Label x:Name="GasAnalyzerPromZonePercent" Content="50%" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center" VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                                </Viewbox>
                            </Border>
                        </StackPanel>
                    </Viewbox>

                    <Viewbox VerticalAlignment="Top" Margin="286,19,367,0" Grid.Column="1" Grid.ColumnSpan="3" Height="59">
                        <StackPanel Orientation="Horizontal">
                            <Label Content="Температура" FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Margin= "0, 0, 10, 0" VerticalContentAlignment="Center" Foreground="Black"/>
                            <Border CornerRadius="12" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Center" Background="#FFE2E1E1" Height="28" Width="78" Margin="0,6">
                                <Viewbox>
                                    <Label x:Name="PromZoneTemp" Content="30 C" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                                </Viewbox>
                            </Border>
                        </StackPanel>
                    </Viewbox>
                </Grid>

                <Grid Grid.Row="1" Grid.Column="0" Margin="-20,46,0,0">
                    <Grid.RowDefinitions>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="425*"></ColumnDefinition>
                        <ColumnDefinition Width="467"></ColumnDefinition>
                        <ColumnDefinition Width="404*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <!--Потолок-->
                    <Viewbox Grid.Column="0" Margin="0,51,0,31">
                        <StackPanel Width="20969" Height="19192">
                            <TextBlock HorizontalAlignment="Center" TextAlignment="Center" FontSize="1215" Text="Имитатор объемного воспламенения" TextWrapping="Wrap" FontWeight="Bold" Width="17149" VerticalAlignment="Center" Margin="2364,300,1456,0" Height="3416"></TextBlock>
                            <Image x:Name="Ceiling" HorizontalAlignment="Center" VerticalAlignment="Top" Source="Images\Объёмное воспламенение_1.png" Cursor="Hand" Height="14068" Margin="3241,-1000,1373,900" Width="16355"/>
                            <Grid Height="2417">
                                <Image x:Name="Img_But_Ceiling" Source="Images\Grey кнопка.png" Margin="5069,-724,10269,850"/>
                                <Button Style="{StaticResource MyButton}" Grid.Column="0" x:Name="Fuse_Ceiling"  VerticalAlignment="Center" FontSize="11" FontFamily="Arial" Click="Fuse_Ceiling_Click" Height="1873" Cursor="Hand" BorderBrush="{x:Null}" FontWeight="Bold" Margin="5268,-446,10325,990">
                                    <Button.Content>
                                        <TextBlock Text="Запальная горелка" TextWrapping="Wrap" FontSize="717" TextAlignment="Center" Width="5371" Height="1854"/>
                                    </Button.Content>
                                </Button>
                                <Image x:Name="Img_But_Ceiling2" Source="Images\Grey кнопка.png" Margin="11077,-712,4153,861" />
                                <Button Style="{StaticResource MyButton}" x:Name="Fuse_Ceiling2"  VerticalAlignment="Center" FontSize="11" FontFamily="Arial" Click="Fuse_Ceiling_Click2" Height="1826" Cursor="Hand" BorderBrush="{x:Null}" FontWeight="Bold" Foreground="Gray" Margin="11196,-508,4174,1099">
                                    <Button.Content>
                                        <TextBlock Text="Основная горелка" FontSize="717" TextWrapping="Wrap" TextAlignment="Center" HorizontalAlignment="Center" Width="5512" Height="1710"/>
                                    </Button.Content>
                                </Button>
                            </Grid>
                        </StackPanel>
                    </Viewbox>

                    <!--Кровать-->
                    <Viewbox Grid.Column="1" Margin="0,51,0,31">
                        <StackPanel Width="7082" Height="6036">
                            <TextBlock HorizontalAlignment="Center" TextAlignment="Center" FontSize="400" Text="Имитатор горящей кровати" TextWrapping="Wrap" FontWeight="Bold" Width="5887" VerticalAlignment="Center" Margin="286,140,149,0" Height="942"></TextBlock>
                            <Image x:Name="Bed" HorizontalAlignment="Center" VerticalAlignment="Top" Source="Images\Кровать_1.png" Cursor="Hand" Height="4699" Margin="438,-300,323,0" Width="6321"/>
                            <Grid Height="2242" Margin="0,-500,0,0">
                                <Image x:Name="Img_But_Bed" Source="Images\Grey кнопка.png" Margin="1493,136,3780,950"/>
                                <Button Style="{StaticResource MyButton}" x:Name="Fuse_Bed"  VerticalAlignment="Center" FontSize="11" FontFamily="Arial" Height="596" Click="Fuse_Bed_Click" Cursor="Hand" BorderBrush="{x:Null}" FontWeight="Bold" Margin="1550,415,3780,1231">
                                    <Button.Content>
                                        <TextBlock Text="Запальная горелка" TextWrapping="Wrap" FontSize="257" TextAlignment="Center" Width="1722" Height="619"/>
