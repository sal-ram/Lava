                                </Button.Content>
                            </Button>
                        </Grid>
                    </Border>

                    <Viewbox VerticalAlignment="Top" Margin="2,19,-139,0" Height="59" Grid.Column="3">
                        <StackPanel Orientation="Horizontal" Width="331" Height="40" >
                            <Label Content="Концентрация газа" FontSize="15" FontWeight="Bold" FontFamily="Arial"  VerticalContentAlignment="Center" Margin= "0, 0, 10, 0" Foreground="Black"/>
                            <Border CornerRadius="12" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Center" Background="#FFE2E1E1" Height="28" Width="78" Margin="0,6">
                                <Viewbox>
                                    <Label x:Name="GasAnalyzerPromZonePercent" Content="50%" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center" VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                                </Viewbox>
                            </Border>
                        </StackPanel>
                    </Viewbox>

                    <Viewbox VerticalAlignment="Top" Margin="286,19,367,0" Grid.Column="1" Grid.ColumnSpan="3" Height="59">
