                                <Button.Content>
                                    <TextBlock Text="Основная горелка" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" HorizontalAlignment="Center" Width="122" Height="38"/>
                                </Button.Content>
                            </Button>
                        </Grid>
                    </Border>
                    <!--Верхний фланец-->
                    <Border Grid.Column="3" Margin="208,181,20,174" BorderBrush="Black" BorderThickness="1" CornerRadius="10">
                        <Grid>
                            <TextBlock HorizontalAlignment="Center" TextAlignment="Center" FontSize="15" TextWrapping="Wrap" FontWeight="Bold" Width="127" VerticalAlignment="Center" Margin="21,10,20,124" Height="22" Padding="0" ScrollViewer.CanContentScroll="True"><Run  Text="Верхний "/><Run Text="фланец"/></TextBlock>
