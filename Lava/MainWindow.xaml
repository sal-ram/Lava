<Window x:Class="Lava.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lava"
        mc:Ignorable="d"
        Title="MainWindow"
        ResizeMode="NoResize"
        WindowStyle="None"
        WindowState="Maximized"
        Width="1680" Height="1050">

    <Window.Resources>
        <Storyboard x:Key="FlashButton" RepeatBehavior="Forever">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="button">
                <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="0.2"/>
                <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="ChangeColor">
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)" Storyboard.TargetName="button">
                <EasingColorKeyFrame KeyTime="0:0:0.5" Value="Red"/>
            </ColorAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="ChangeColor2">
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)" Storyboard.TargetName="button">
                <EasingColorKeyFrame KeyTime="0:0:0.5" Value="White"/>
            </ColorAnimationUsingKeyFrames>
        </Storyboard>
    </Window.Resources>

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Images/Интерфейс Лава2.png"/>
        </Grid.Background>

        <Grid VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0,10,10,993" Height="47" Width="137">
            <Button Style="{StaticResource MyButton}" Content="-" Cursor="Hand" FontSize="16" MinWidth="25" MinHeight="25" VerticalAlignment="Top" Background="#FF959393" Foreground="White" RenderTransformOrigin="0.5,0.5" FontWeight="Bold" FontStyle="Italic" Click="Button_Click_1" Height="40" Width="40"/>
            <Button Style="{StaticResource MyButton}" Content="X" HorizontalAlignment="Right" Cursor="Hand" FontSize="16" MinWidth="25" MinHeight="25" VerticalAlignment="Top" Background="#FFE64343" Foreground="White" Click="Button_Click" Width="40" Height="40"/>
        </Grid>

        <Grid Margin="10,57,10,10">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="101*"/>
                <ColumnDefinition Width="336*"/>
            </Grid.ColumnDefinitions>

            <Grid.Background>
                <ImageBrush ImageSource="Images/Интерфейс Лава4.png"/>
            </Grid.Background>

            <Grid Grid.Column="0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="372*"></RowDefinition>
                    <RowDefinition Height="290*"></RowDefinition>
                    <RowDefinition Height="557*"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Viewbox>
                    <StackPanel Orientation="Vertical">
                        <Border x:Name="AlarmBorder" CornerRadius="12" Grid.Row="0" BorderThickness="2" VerticalAlignment="Top" RenderTransformOrigin="0.468,0.361" Background="#FF5AC155" Height="46" Margin ="10, 6, 2, 1" >
                            <Viewbox>
                                <Label x:Name="AlarmText" Content="Состояние: исправно" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  Margin="-2" VerticalAlignment="Center" FontSize="13" FontFamily="Arial" FontWeight="Bold" Foreground="White" Height="38" BorderBrush="#FF555555" Background="{x:Null}"/>
                            </Viewbox>
                        </Border>
                        <Border Margin ="10, 0, 2, 4" CornerRadius="12" Grid.Row="0" BorderThickness="2,3,2,2" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top"  Background="#FFD82F2F" Height="46">
                            <Viewbox>
                                <Label Content="Сброс времени тренировки" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center"  FontSize="13" FontFamily="Arial" FontWeight="Bold" Foreground="White" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>
                        <Label Content="Время:" FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Foreground="Black" Height="26"/>
                        <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top" Background="#FFE2E1E1" Height="27" Margin="100, 0" Width="78">
                            <Viewbox>
                                <Label x:Name="Time" Content="11:05" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="20" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>

                        <Label Grid.Row="0" Content="Время тренировки:" HorizontalContentAlignment="Center" FontSize="15" FontWeight="Bold" FontFamily="Arial" Foreground="Black"/>
                        <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top"  Background="#FFE2E1E1" Height="27" Margin="70, 0">
                            <Viewbox>
                                <Label x:Name="TimeTraining" Content="00:01:05" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center"  FontSize="20" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>
                    </StackPanel>
                </Viewbox>

                <Viewbox Grid.Row="1">
                    <StackPanel Orientation="Vertical" Grid.Row="1" VerticalAlignment="Center"  Margin="10,40,10,0">
                        <Label Content="Температура" FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Foreground="Black"/>
                        <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top" Background="#FFE2E1E1" Height="27" Margin="100, 0" Width="78">
                            <Viewbox>
                                <Label x:Name="GazhranZoneTemp" Content="35 C" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>

                        <Label Content="Концентрация газа" FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Foreground="Black"/>
                        <Border CornerRadius="12" Grid.Row="0" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Top" Background="#FFE2E1E1" Height="27" Margin="100, 0" Width="78">
                            <Viewbox>
                                <Label x:Name="GasAnalyzerGazohranZonePercent" Content="50 %" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                            </Viewbox>
                        </Border>
                    </StackPanel>
                </Viewbox>

                <Viewbox Grid.Row="2" VerticalAlignment="Top" Margin="10,65,10,0">
