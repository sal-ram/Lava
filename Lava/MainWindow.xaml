                                <Button.Content>
                                    <TextBlock Text="Запальная горелка" TextWrapping="Wrap" FontSize="17" TextAlignment="Center" Width="122"/>
                                </Button.Content>
                            </Button>

                            <Image x:Name="Img_But_Flange2_High" Source="Images\Grey кнопка.png" Margin="21,56,20,-38"/>
