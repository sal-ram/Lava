                    </Viewbox>
                    <!--Нижний фланец-->
                    <Border Grid.Column="3" Margin="208,355,20,7" BorderBrush="Black" BorderThickness="1" CornerRadius="10">
                        <Grid>
                            <TextBlock HorizontalAlignment="Center" TextAlignment="Center" FontSize="15" TextWrapping="Wrap" FontWeight="Bold" Width="128" VerticalAlignment="Center" Margin="18,12,17,117" Height="22" Padding="0" ScrollViewer.CanContentScroll="True"><Run Text="Нижний "/><Run Text="фланец"/></TextBlock>
                            <Image x:Name="Img_But_Flange_Low" Source="Images\Grey кнопка.png" Margin="21,28,17,34"/>
