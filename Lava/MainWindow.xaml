                                    <TextBlock Text="Запальная горелка" TextWrapping="Wrap" FontSize="17" TextAlignment="Center" Width="125" Height="46"/>
                                </Button.Content>
                            </Button>

                            <Image x:Name="Img_But_Flange2_Low" Source="Images\Grey кнопка.png" Margin="21,64,17,-28"/>
