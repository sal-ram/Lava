                            <Label Content="Температура" FontSize="15" FontWeight="Bold" FontFamily="Arial" HorizontalContentAlignment="Center" Margin= "0, 0, 10, 0" VerticalContentAlignment="Center" Foreground="Black"/>
                            <Border CornerRadius="12" BorderBrush="Black" BorderThickness="1.5" RenderTransformOrigin="0.468,0.361" VerticalAlignment="Center" Background="#FFE2E1E1" Height="28" Width="78" Margin="0,6">
                                <Viewbox>
                                    <Label x:Name="PromZoneTemp" Content="30 C" HorizontalAlignment="Center" HorizontalContentAlignment ="Center" VerticalContentAlignment="Center"  VerticalAlignment="Center" FontSize="22" FontFamily="Arial" FontWeight="Bold" Foreground="Black" Height="38" BorderBrush="#FF555555" Background="{x:Null}" Margin="-2"/>
                                </Viewbox>
                            </Border>
